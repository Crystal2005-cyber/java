import.java.util.*;
 public static int gcd(int a,int b){
   if(b==0) return a;
    return gcd(b,a%b);
 }
 public static void main(string [] args){
    Scanner sc= new scanner(System.in);
    int a=sc.nextInt();
    int count=0;
    for (int i=0; i<=0;i++);
     if(gcd(i,0)==1){
     count++;
     }
   }
  system.out.println(count);
 }
}
